{
  "name": "@emptystream/frontend-react",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "type": "module",
  "private": "true",
  "scripts": {
    "lint": "eslint --ignore-path ../../.gitignore --ext .js,.ts,.tsx ./src",
    "lint:fix": "eslint --ignore-path ../../.gitignore --ext .js,.ts,.tsx ./src --fix",
    "format": "prettier --ignore-path ../../.gitignore --write \"**/*.+(js|ts|json|tsx)\"",
    "prebuild": "npm run lint",
    "build": "tsc --build && vite build",
    "dev": "vite",
    "start": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@mantine/core": "^7.6.2",
    "@mantine/dropzone": "^7.6.2",
    "@mantine/hooks": "^7.6.2",
    "@mantine/modals": "^7.6.2",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^2.0.1",
    "@tabler/icons-react": "^3.0.1",
    "howler": "^2.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-spring": "^9.7.3",
    "react-use-websocket": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/howler": "^2.2.11",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-config-airbnb": "^19.0.4",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  }
